    <form action="/them" method="post">
        <div class="form-group">
          <label for="ten">Tên :</label>
          <input type="text" class="form-control" name="ten" id="" aria-describedby="helpId" placeholder="">
          <small id="helpId" class="form-text text-muted">Nhập tên vào khung nha</small>
        </div>    
        <div class="form-group">
          <label for="dt">Điện thoại: </label>
          <input type="text" class="form-control" name="dt" id="" aria-describedby="helpId" placeholder="">
          <small id="helpId" class="form-text text-muted">Nhập điện thoại vào khung nha</small>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>

    </form>
</div>

